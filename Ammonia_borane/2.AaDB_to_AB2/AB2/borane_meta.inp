&GLOBAL
  PRINT_LEVEL LOW
  WALLTIME 1000000000
   PROJECT test_1 
   !RUN_TYPE WFN_OPT
   !RUN_TYPE GEO_OPT
   RUN_TYPE MD
   SEED 300

&END GLOBAL

&EXT_RESTART
EXTERNAL_FILE test_1-1.restart
RESTART_POS T
RESTART_VEL T
RESTART_THERMOSTAT T
RESTART_COUNTERS OFF
&END EXT_RESTART

&MOTION

  &GEO_OPT
  MAX_ITER 30000
  &END GEO_OPT


  &MD

   COMVEL_TOL 0.000005
   ANGVEL_TOL 0.000005	
   ANGVEL_ZERO
   ENSEMBLE NVT
   STEPS 100000
   TIMESTEP 1
   TEMPERATURE 400.0

!   TEMP_TOL 40

!   &PRINT
!    &ENERGY
!       EACH 5
!    &END ENERGY
!    &RESTART
!       EACH 100
!    &END RESTART
!   &END PRINT

    &THERMOSTAT
      TYPE CSVR
      &CSVR
        TIMECON 40.
      &END CSVR
    &END THERMOSTAT

  
 &END MD

  &PRINT
   &RESTART
    &EACH
     MD 1
    &END EACH
   &END RESTART
  &END PRINT

  &FREE_ENERGY
	&METADYN
            USE_PLUMED .TRUE.
            PLUMED_INPUT_FILE ./plumed.dat
        &END METADYN
  &END FREE_ENERGY

!   &FREE_ENERGY
!    &METADYN
!      WELL_TEMPERED
!      WTGAMMA 10
!      DO_HILLS
!      NT_HILLS 100
!      WW 0.0005
!      &METAVAR
!       COLVAR 1
!       SCALE 0.08

!      &END METAVAR
!      &PRINT
!       &COLVAR
!        COMMON_ITERATION_LEVELS 10000
!       &END COLVAR
!       &HILLS
!        COMMON_ITERATION_LEVELS 10000
!       &END HILLS
!      &END PRINT

!    &END METADYN
!  &END FREE_ENERGY



&END MOTION

&FORCE_EVAL
  METHOD Quickstep

  &DFT
    WFN_RESTART_FILE_NAME test_1-RESTART.wfn
    CHARGE 0
    MULTIPLICITY 1
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
  
    &MGRID
      CUTOFF 300
    &END MGRID
    
    &QS      
      MAP_CONSISTENT
    &END QS

    !&POISSON
    !PERIODIC NONE
    !POISSON_SOLVER MT
    !&END POISSON

    &SCF

!      SCF_GUESS ATOMIC
      SCF_GUESS RESTART
      EPS_SCF 1.e-6
      MAX_SCF 50
      &OT
        MINIMIZER DIIS
        N_DIIS 7
        PRECONDITIONER FULL_KINETIC
      &END OT

       &OUTER_SCF
        TYPE NONE
        OPTIMIZER NONE
        EPS_SCF 1.0E-6
        MAX_SCF 25

       &END OUTER_SCF 
    &END SCF
    
    &XC
     &XC_FUNCTIONAL PBE
     &END XC_FUNCTIONAL

     &XC_GRID
      XC_SMOOTH_RHO NN50
      XC_DERIV NN50_SMOOTH
     &END XC_GRID

    &END XC

    &PRINT
     &MULLIKEN ON
	  FILENAME ./mulliken.dat
 	  &EACH
	   MD 500
	  &END EACH
     &END MULLIKEN
    &END PRINT

  &END DFT

